{
  "name": "@rainbowprotocol/sdk",
  "version": "1.0.0",
  "main": "dist/index.js",
  "files": [
    "dist"
  ],
  "repository": "",
  "author": "Rainbow Protocol",
  "license": "MIT",
  "scripts": {
    "build": "yarn compile && yarn generate-types && rollup -c && cp ./src/ethers/*.d.ts ./dist/ethers/",
    "compile": "truffle compile --all",
    "test": "scripts/test.sh",
    "size": "truffle run contract-size",
    "deploy": "truffle migrate --f 1 --to 1 --network mainnet",
    "testAdmin": "truffle exec test/testAdmin.js --network heco",
    "testAdminETH": "truffle exec test/testAdmin.js --network mainnet",
    "testAdminRinkeby": "truffle exec test/testAdmin.js --network rinkeby",
    "addTokens": "truffle exec test/addTokens.js --network mainnet",
    "console": "truffle console --network heco",
    "testRainbow": "truffle exec test/testRainbow.js --network heco",
    "testRainbowRinkeby": "truffle exec test/testRainbow.js --network rinkeby",
    "testProxy": "truffle exec test/testProxy.js --network heco",
    "testSuShiProxy": "truffle exec test/testSuShiProxy.js --network mainnet",
    "migrate": "truffle migrate --network heco",
    "coverage": "truffle run coverage --network development",
    "publish:package": "npm publish --access public",
    "remixd": "remixd -s . --remix-ide https://remix.ethereum.org",
    "generate-types": "yarn generate-types:ethers",
    "generate-types:ethers": "npx typechain \"build/contracts/*.json\" --target=ethers-v5 --outDir src/ethers",
    "deploy-local": "truffle migrate && truffle exec scripts/create_rainbow.js --network ${NETWORK:-development}",
    "deploy-kovan": "NETWORK=kovan yarn deploy-local",
    "deploy-rinkeby": "NETWORK=rinkeby yarn deploy-local",
    "deploy-mainnet": "truffle migrate && truffle exec scripts/mainnet_create_markets.js --network mainnet",
    "transfer-ownership-local": "truffle exec scripts/transfer_ownership.js --network ${NETWORK:-development}",
    "transfer-ownership-rinkeby": "NETWORK=rinkeby yarn transfer-ownership-local",
    "transfer-ownership-mainnet": "NETWORK=mainnet yarn transfer-ownership-local",
    "clean:dependencies": "rm -rf ./node_modules && rm -rf ./*/**/node_modules",
    "clean:dist": "rm -rf ./*/**/dist && rm -rf ./packages/contracts/build && rm -rf ./packages/contracts/src/ethers",
    "clean": "yarn clean:dependencies && yarn clean:dist"
  },
  "peerDependencies": {
    "ethers": ">5.0.0"
  },
  "dependencies": {
    "@babel/core": "^7.12.3",
    "@chainlink/contracts": "^0.0.11",
    "@nomiclabs/hardhat-ethers": "^2.0.2",
    "@nomiclabs/hardhat-waffle": "^2.0.1",
    "@openzeppelin/contracts": "^4.2.0",
    "@openzeppelin/test-helpers": "^0.5.9",
    "@openzeppelin/upgrades": "^2.8.0",
    "@rollup/plugin-babel": "^5.2.1",
    "@rollup/plugin-commonjs": "^16.0.0",
    "@rollup/plugin-node-resolve": "^10.0.0",
    "@truffle/hdwallet-provider": "^1.1.0",
    "@typechain/ethers-v5": "3.0.0",
    "@uniswap/v3-core": "^1.0.0",
    "bignumber.js": "^9.0.1",
    "bn.js": "^4.11.0",
    "chai": "^4.3.4",
    "dotenv": "^8.2.0",
    "ethers": "5.0.7",
    "ganache-cli": "^6.9.1",
    "graphql": "^15.4.0",
    "graphql-request": "^3.3.0",
    "hardhat": "^2.4.1",
    "hardhat-deploy-ethers": "^0.3.0-beta.10",
    "husky": "^4.3.0",
    "prettier": "2.1.1",
    "pretty-quick": "^3.1.0",
    "rollup": "^2.33.1",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "rollup-plugin-typescript2": "^0.29.0",
    "solidity-coverage": "^0.7.12",
    "truffle": "5.1.49",
    "truffle-contract-size": "^2.0.1",
    "typechain": "4.0.0",
    "typescript": "^4.3.5",
    "web3": "^1.3.0",
    "yargs": "^17.1.0"
  },
  "devDependencies": {
    "@nomiclabs/hardhat-truffle5": "^2.0.0",
    "@nomiclabs/hardhat-web3": "^2.0.0",
    "hardhat-abi-exporter": "^2.2.1",
    "hardhat-contract-sizer": "^2.0.3",
    "hardhat-deploy": "^0.8.9",
    "hardhat-gas-reporter": "^1.0.4",
    "hardhat-spdx-license-identifier": "^2.0.3",
    "hardhat-tracer": "^1.0.0-alpha.6"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick 'src/**/*.{ts,tsx,js,jsx,md}' --staged"
    }
  }
}
